{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "v-defona-adf"
		},
		"integrationRuntime_v-defona-azuressis-integrationRuntime": {
			"type": "string",
			"metadata": "Integration runtime name",
			"defaultValue": "v-defona-azuressis-integrationRuntime"
		},
		"integrationRuntime_v-defona-integrationRuntime": {
			"type": "string",
			"metadata": "Integration runtime name",
			"defaultValue": "v-defona-integrationRuntime"
		},
		"AzureBlobStorageLinkedService_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'AzureBlobStorageLinkedService'"
		},
		"AzureCosmosDbMongoDbLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureCosmosDbMongoDbLinkedService'"
		},
		"AzureSqlDatabaseLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseLinkedService'"
		},
		"AzureSqlDatabaseLinkedService__connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseLinkedService_'"
		},
		"AzureSynapseAnalyticsLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSynapseAnalyticsLinkedService'"
		},
		"Office3651_LinkedService_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'Office3651_LinkedService'"
		},
		"AzureBlobStorageLinkedService_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://vdefonastorageblobgen2.blob.core.windows.net"
		},
		"AzureBlobStorageLinkedService_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "72f988bf-86f1-41af-91ab-2d7cd011db47"
		},
		"AzureBlobStorageLinkedService_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "a86f132a-dd4d-456c-9099-05e5a14e9773"
		},
		"AzureCosmosDbMongoDbLinkedService_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "v-defona-01"
		},
		"Office3651_LinkedService_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "a86f132a-dd4d-456c-9099-05e5a14e9773"
		},
		"v-defona-azuressis-integrationRuntime_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint": {
			"type": "string",
			"defaultValue": "v-defona-sqlserver.database.windows.net"
		},
		"v-defona-azuressis-integrationRuntime_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName": {
			"type": "string",
			"defaultValue": "v-defona"
		},
		"v-defona-azuressis-integrationRuntime_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword": {
			"type": "secureString"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"integrationRuntime_v-defona-azuressis-integrationRuntime": {
						"value": "[parameters('integrationRuntime_v-defona-azuressis-integrationRuntime')]"
					},
					"integrationRuntime_v-defona-integrationRuntime": {
						"value": "[parameters('integrationRuntime_v-defona-integrationRuntime')]"
					},
					"AzureBlobStorageLinkedService_servicePrincipalKey": {
						"value": "[parameters('AzureBlobStorageLinkedService_servicePrincipalKey')]"
					},
					"AzureSqlDatabaseLinkedService_connectionString": {
						"value": "[parameters('AzureSqlDatabaseLinkedService_connectionString')]"
					},
					"AzureSynapseAnalyticsLinkedService_connectionString": {
						"value": "[parameters('AzureSynapseAnalyticsLinkedService_connectionString')]"
					},
					"Office3651_LinkedService_servicePrincipalKey": {
						"value": "[parameters('Office3651_LinkedService_servicePrincipalKey')]"
					},
					"AzureCosmosDbMongoDbLinkedService_connectionString": {
						"value": "[parameters('AzureCosmosDbMongoDbLinkedService_connectionString')]"
					},
					"AzureSqlDatabaseLinkedService__connectionString": {
						"value": "[parameters('AzureSqlDatabaseLinkedService__connectionString')]"
					},
					"AzureBlobStorageLinkedService_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('AzureBlobStorageLinkedService_properties_typeProperties_serviceEndpoint')]"
					},
					"AzureBlobStorageLinkedService_properties_typeProperties_tenant": {
						"value": "[parameters('AzureBlobStorageLinkedService_properties_typeProperties_tenant')]"
					},
					"AzureBlobStorageLinkedService_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureBlobStorageLinkedService_properties_typeProperties_servicePrincipalId')]"
					},
					"Office3651_LinkedService_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('Office3651_LinkedService_properties_typeProperties_servicePrincipalId')]"
					},
					"v-defona-azuressis-integrationRuntime_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint": {
						"value": "[parameters('v-defona-azuressis-integrationRuntime_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint')]"
					},
					"v-defona-azuressis-integrationRuntime_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName": {
						"value": "[parameters('v-defona-azuressis-integrationRuntime_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName')]"
					},
					"v-defona-azuressis-integrationRuntime_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword": {
						"value": "[parameters('v-defona-azuressis-integrationRuntime_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword')]"
					},
					"AzureCosmosDbMongoDbLinkedService_properties_typeProperties_database": {
						"value": "[parameters('AzureCosmosDbMongoDbLinkedService_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}